{
  "name": "bcbp",
  "version": "0.0.0-semantically-released",
  "description": "Encoding/decoding library for the IATA Bar Coded Boarding Pass",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",

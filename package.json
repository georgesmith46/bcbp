    "test:smoke": "jest test/smoke.test.js",
    "release": "npx semantic-release",
    "release-dry-run": "npx semantic-release-github-pr --debug"
  },
  "author": {
    "name": "George Smith <georges8899@gmail.com>"
  },
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {

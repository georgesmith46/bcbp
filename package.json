{
	"name": "bcbp",
	"version": "0.0.0-semantically-released",
	"description": "Encoding/decoding library for the IATA Bar Coded Boarding Pass",
	"main": "./dist/index.umd.js",
	"module": "./dist/index.esm.js",
	"repository": "github:georgesmith46/bcbp",
	"scripts": {
		"build": "rollup -c",
		"test": "jest --testPathIgnorePatterns=\"/node_modules/\", \"/test/\"",
		"test:smoke": "jest test/smoke.test.js",
		"release": "npm run build && npx semantic-release",
		"release-dry-run": "npx semantic-release-github-pr"
	},
	"author": {
		"name": "George Smith <georges8899@gmail.com>"
	},
	"license": "MIT",
	"sideEffects": false,
	"devDependencies": {
		"@babel/core": "^7.12.13",
		"@babel/plugin-transform-runtime": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@rollup/plugin-babel": "^5.2.3",
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.1.1",
		"babel-jest": "^26.6.3",
		"cz-conventional-changelog": "^3.3.0",
		"date-fns": "^2.16.1",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"rollup": "^2.38.4",
		"semantic-release": "^17.3.7",
		"semantic-release-github-pr": "^6.0.1"
	},
	"keywords": [
		"iata",
		"boarding pass",
		"barcode",
		"encode",
		"decode"
	],
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
